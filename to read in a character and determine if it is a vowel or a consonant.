#include<iostream>
using namespace std;
int main()
{
  char a;
  cout<<"Enter character\n";
  cin>>a;
  if(a=='a'||a=='e'||a=='i'||a=='o'||a=='u')
    cout<<"It is a vowel";
  else if(a>='a'&&a<='z')
    cout<<"It is consonant";
  else if(a>='0'&&a<='9')
    cout<<"It is a digit";
  else if(a==' ')
    cout<<"It is space";
}
